   <div class="container mt-5">
    <div class="row">
        {{#each products}}
        <div class="col-md-3 col-sm-6">
    <div class="card" style="width: 18rem;">
  <img
  src="{{this.image}}"
  class="card-img-top" 
   alt="card image cap"/>
  <div class="card-body">
    <h2 class="card-title">{{this.name}}</h2>
    <h4 class="card-title">{{this.Processor}}</h4>
    <h5 class="card-text">{{this.os}}</h5>
    <h2>{{this.rate}}</h2>
    <a href="#" class="btn btn-primary">Add to Cart</a>
  </div>
</div>
</div>
{{/each}}
</div>
</div>
</section>
  